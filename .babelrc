{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "targets": {
          "node": "current",
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver", 
      {
        "root": ["."],
        "alias": {
          "@": "./src",
          "Protos": "./proto",
          "ABIs": "./abi"
        }
      }
    ],
    "add-module-exports" 
  ]
}
